import java.util.Scanner;
public class hello
{
	public static void main(String[] args) {
	   Scanner scan = new Scanner(System.in);
       int age=scan.nextInt();
       int applicantage=scan.nextInt();
       int employment=scan.nextInt();
       int income=scan.nextInt();
       if(age>=21 && age<=60 && applicantage>18 && (employment==1 || employment==2 || employment==3 || employment== 4) && income>=300000){
       System.out.println("Yes.you are eligible for SBI credit cards");
       }
      else{
         System.out.println("NO.You are not elligible for SBI credit cards");
      }
   }
}
