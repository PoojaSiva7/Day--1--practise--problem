import java.util.Scanner;
public class hello
{
	public static void main(String[] args) {
	   Scanner scan = new Scanner(System.in);
       int num=scan.nextInt();
      if(num%3==0 && num%5==0){
        System.out.println("Yes");
      }
      else{
        System.out.println("no");
      }
      
  }
}
